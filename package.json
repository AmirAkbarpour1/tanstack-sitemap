  "license": "MIT",
  "devDependencies": {
    "@tanstack/eslint-config": "^0.2.0",
    "@types/node": "^22.15.29",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.5.3",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "vite": "^6.3.5",
    "xmlbuilder2": "^3.1.1"
