{
  "name": "tanstack-sitemap",
  "version": "1.1.6",
  "description": "A sitemap builder written in TypeScript for Tanstack start",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest",
    "build": "tsup",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "tanstack",
    "sitemap",
    "builder",
    "typescript",
    "seo",
    "vite"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Amir Akbarpour",
  "license": "MIT",
  "devDependencies": {
    "@tanstack/eslint-config": "^0.2.0",
    "@types/node": "^22.15.29",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
