{
  "name": "tanstack-sitemap",
  "version": "1.0.0",
  "description": "A  sitemap builder written in TypeScript for Tanstack start",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
